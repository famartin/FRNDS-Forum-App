<html>
<head>
<%- include('head'); -%>
</head>
<body>
<%- include('nav'); -%>
<div class="container">
	<br>
	<h2 class="raleway">Chat</h2>
	<h5 id="chat-username" class="raleway" value="<%= username %>"><%= username %></h5>
	<div id="chat">
		<div id="chat-window">
			<div id="output">
			</div>
			<div id="feedback">
			</div>
		</div>
	
		<form action="">
			<input id="m" autocomplete="off" placeholder="say something..." /><button id="chat-button">Send</button>
		</form>
	</div>
		<script src="/socket.io/socket.io.js"></script>
		<script src="https://code.jquery.com/jquery-1.11.1.js"></script>
		<script src="/js/chat.js"></script>
</div>
</body>
</html>
